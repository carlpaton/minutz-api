{
    "dotnet-test-explorer.testProjectPath" : "src/Tests", 
    "dotnet-test-explorer.useTreeView": true, 
    "dotnet-test-explorer.build": true, 
    "dotnet-test-explorer.restore": true, 
    "dotnet-test-explorer.enableTelemetry": true, 
    "cSpell.words": [ 
      "bitbucket", 
      "buildagent", 
      "dcproj", 
      "minutz" 
    ], 
    "csharpfixformat.style.braces.onSameLine": false, 
    "csharp.testsCodeLens.enabled": true, 
    "csharpfixformat.sort.usings.enabled": true ,
    "run": {
        "commands": [
            {
                "name": "Build",
                "singleInstance": true,
                "color": "#ffff",
                "command": "dotnet build src/Api/Api.csproj",
            },
        ]
    }
}