    var minutz = {
      functions: {
        get: {
          value: function (control) { return $('#' + control).val(); },
          datepicker: function (control) { return $('#' + control).datepicker().val(); },
          text: function (control) { return $('#' + control).text(); }
        },
        set: {
          value: function (control, value) { $('#' + control).val(value); },
