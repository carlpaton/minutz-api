          return minutz.functions.s4() + minutz.functions.s4() + '-' + minutz.functions.s4() + '-' + minutz.functions.s4() + '-' + minutz.functions.s4() + '-' + minutz.functions.s4() + minutz.functions.s4() + minutz.functions.s4();
        },
        findIndex: function (collection, id) {
          for (var q = 0; q < collection.length; q++) {
            var check = collection[q];
            if (check.Id == id) {
              return q;
            }
          }
        },
        find: function (collection, id) {
          for (var q = 0; q < collection.length; q++) {
            var check = collection[q];
            if (check.Id == id) {
