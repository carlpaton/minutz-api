    //minutz.functions.updateAgendaTextToModel();
    minutz.functions.updateMainModelProperties();

    if (minutz.functions.saveAjax()) {
      minutz.functions.notifyMe(MinutzTitle, "The updates were saved successfully.", "fa fa-info");

